<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" x:Class="App.Views.CommissionPreparer.CommissionPreparerMenuPage">
    <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
        <Button Content="Управление комиссиями" 
                Command="{Binding OnClickBtnCommissionControl}"/>
        <Button Content="Управление членами комиссий" 
                Command="{Binding OnClickBtnCommissionMemberControl}"/>
        <Button Content="Управление инвентаризациями" 
                Command="{Binding OnClickBtnInventoryControl}"/>
        <Button Content="Управление проверямыми единицами" 
                Command="{Binding OnClickBtnInspectedUnitControl}"/>
    </StackPanel>
</UserControl>
