<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" x:Class="App.Views.CommissionMember.CommissionMemberMenuPage">
    <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
        <Button Content="Управление аудиториями" 
                Command="{Binding OnClickBtnAudienceControl}"/>
        <Button Content="Управление оборудованием" 
                Command="{Binding OnClickBtnEquipmentControl}"/>
        <Button Content="Управление типами оборудоания" 
                Command="{Binding OnClickBtnEquipmentTypeControl}"/>
        <Button Content="Управление единицами оборудования" 
                Command="{Binding OnClickBtnEquipmentUnitControl}"/>
    </StackPanel>
</UserControl>
